@model IEnumerable<HANDS_ON_LAB.Models.Product>
    <h2>
        Product list as on: @DateTime.Now.ToString()
        @{ 
            foreach (var p in Model)
            {
                <h2>Product Id: @p.ProductId</h2>
                <h3>Product Name: @p.ProductName</h3>
                <h3>Price: @p.Price</h3>
                <hr />
            }
        }
    </h2>
